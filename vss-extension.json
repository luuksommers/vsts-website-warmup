{
    "manifestVersion": 1,
    "id": "website-warmup",
    "name": "Website Warmup",
    "version": "0.4.1",
    "publisher": "luuksommers",
    "public": true,
    "helpMarkDown": "[More Information](https://github.com/luuksommers/vsts-website-warmup)",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "This extension does a website warmup. When the site is not available, it throws an exception.",
    "categories": [
        "Build and release"
    ],
    "tags": [
        "azure",
        "webapp",
        "warmup",
        "iis"
    ],
    "screenshots": [
        {
            "path": "images/screenshot-1.png"
        }
    ],
    "content": {
        "details": {
            "path": "readme.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/luuksommers/vsts-website-warmup"
        },
        "support": {
            "uri": "https://github.com/luuksommers/vsts-website-warmup"
        }
    },
    "branding": {
        "color": "rgb(6, 6, 6)",
        "theme": "dark"
    },
    "icons": {
        "default": "images/logo.png"
    },
    "files": [
        {
            "path": "website-warmup"
        }
    ],
    "contributions": [
        {
            "id": "website-warmup",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "website-warmup"
            }
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/luuksommers/vsts-website-warmup"
    }
}